{
  "name": "bmixjs",
  "private": true,
  "description": "bmixjs",
  "keywords": [
    "bmix"
  ],
  "homepage": "https://github.com/bmixjs/bmix#readme",
  "bugs": {
    "url": "https://github.com/bmixjs/bmix/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmixjs/bmix.git"
  },
  "license": "MIT",
  "author": "Zack <mwl@live.com> (https://github.com/loyep)",
  "scripts": {
    "bootstrap": "esno scripts/bootstrap.ts",
    "build": "pnpm -r --filter ./packages run build",
    "build:deps": "esno scripts/bundleDeps.ts",
    "clean": "rimraf packages/**/compiled && rimraf packages/**/dist",
    "dev": "pnpm -r --filter ./packages run dev",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "esno scripts/postinstall.ts",
    "jest": "jest",
    "lint": "prettier --write .",
    "pre:publish": "esno scripts/publish.ts",
    "prepare": "husky install",
    "release": "esno scripts/release.ts"
  },
  "lint-staged": {
    "*.{jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@playwright/test": "1.21.0",
    "@pnpm/lockfile-file": "5.0.0",
    "@pnpm/logger": "4.0.0",
    "@swc/core": "1.2.171",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.24",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "@types/resolve": "1.20.1",
    "@types/rimraf": "3.0.2",
    "bmix": "workspace:*",
    "bmix-compiled": "0.0.7",
    "bmix-env": "workspace:*",
    "esbuild": "0.14.36",
    "esbuild-jest": "0.5.0",
    "esno": "0.14.1",
    "expect-playwright": "0.8.0",
    "fast-glob": "3.2.11",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-playwright-preset": "1.7.0",
    "lerna": "4.0.0",
    "lint-staged": "12.3.8",
    "only-allow": "1.1.0",
    "playwright-chromium": "1.21.0",
    "prettier": "2.6.2",
    "prettier-plugin-organize-imports": "2.3.4",
    "prettier-plugin-packagejson": "2.2.17",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "resolve": "1.22.0",
    "rimraf": "3.0.2",
    "standard-version": "9.3.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.3",
    "uglify-js": "3.15.4",
    "webpack": "5.72.0",
    "zx": "4.3.0"
  },
  "packageManager": "pnpm@6.32.8",
  "engines": {
    "node": ">=14",
    "pnpm": ">=6.20.0"
  }
}
